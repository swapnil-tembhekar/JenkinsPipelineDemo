pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Building the code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the code'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the code'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing the code'
            }
        }
    }
}
